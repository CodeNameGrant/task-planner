{
  "name": "e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/e2e/src",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "task-planner-poc:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "task-planner-poc:serve:production"
        },
        "ci": {
          "devServerTarget": "task-planner-poc:serve-static"
        }
      }
    }
  },
  "implicitDependencies": ["task-planner-poc"],
  "tags": []
}
